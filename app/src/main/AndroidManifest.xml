<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yoloswag.alex.seetitel" >
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/whistle64"
        android:label="@string/app_name"
        android:theme="@style/AppTheme"
        android:name="com.yoloswag.alex.seetitel.AppController">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AudioActivity"
            android:label="AudioActivity"
            android:parentActivityName="com.yoloswag.alex.seetitel.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.yoloswag.alex.seetitel.MainActivity" />
        </activity>
        <activity
            android:name=".TextActivity"
            android:label="TextActivity"
            android:parentActivityName="com.yoloswag.alex.seetitel.MainActivity" >
            <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value="com.yoloswag.alex.seetitel.MainActivity" />
        </activity>
        <activity
            android:name=".ImageActivity"
            android:label="ImageActivity"
            android:parentActivityName="com.yoloswag.alex.seetitel.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.yoloswag.alex.seetitel.MainActivity" />
        </activity>
    </application>

</manifest>
